<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="gui.auth.CaptchaController">
   <children>
      <AnchorPane layoutX="100.0" layoutY="61.0" prefHeight="326.0" prefWidth="374.0">
         <children>
            <Label fx:id="captchahere" alignment="CENTER" contentDisplay="CENTER" layoutY="69.0" prefHeight="46.0" prefWidth="400.0" text="captcha" textFill="#19760d" textOverrun="CENTER_ELLIPSIS" />
            <Label layoutX="80.0" layoutY="160.0" prefHeight="37.0" prefWidth="110.0" text="Enter Captcha :" />
            <Label fx:id="messagebtn" alignment="CENTER" contentDisplay="CENTER" layoutX="-1.0" layoutY="339.0" prefHeight="20.0" prefWidth="400.0" text="Please Enter Captcha" textAlignment="CENTER" />
            <TextField fx:id="textarea" layoutX="191.0" layoutY="166.0" prefHeight="26.0" prefWidth="135.0" />
            <Button fx:id="checkbtn" layoutX="82.0" layoutY="210.0" mnemonicParsing="false" onAction="#captchachecker" prefHeight="38.0" prefWidth="246.0" text="check" />
            <Button fx:id="newcaptchabtn" alignment="CENTER" contentDisplay="CENTER" layoutX="82.0" layoutY="258.0" mnemonicParsing="false" onAction="#setcaptcha" prefHeight="38.0" prefWidth="246.0" text="new captcha" textAlignment="RIGHT" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
